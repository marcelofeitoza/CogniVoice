generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../database/cognivoice.db"
}

model User {
  id        String   @id
  email     String   @unique
  name      String?
  password  String
  mode      String?  @default("")
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
